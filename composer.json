{
    "name": "qsomazzi/portal-v2",
    "license": "MIT",
    "type": "project",
    "description": "Portal v2",
    "autoload": {
        "psr-4": { "": "src/", "SymfonyStandard\\": "app/SymfonyStandard/" }
    },
    "require": {
        "php": ">=5.3.9",

        "symfony/symfony": "2.7.*",
        "symfony/assetic-bundle": "~2.3",
        "symfony/swiftmailer-bundle": "~2.3",
        "symfony/monolog-bundle": "~2.4",

        "doctrine/orm": "~2.2,>=2.2.3,<2.5",
        "doctrine/dbal": "<2.5",
        "doctrine/doctrine-bundle": "~1.4",

        "sensio/distribution-bundle": "~4.0",
        "sensio/framework-extra-bundle": "~3.0,>=3.0.2",

        "knplabs/github-api": "~1.4",
        "knplabs/knp-menu-bundle": "2.0.x-dev",
        "knplabs/knp-markdown-bundle": "~1.3.0",

        "incenteev/composer-parameter-handler": "~2.0",
        "cocur/slugify": "~1.2",
        "jms/serializer-bundle": "~0.11",

        "sonata-project/admin-bundle": "dev-master#a0801f1a9af944a2164530d4be43aade53ab6a64",
        "sonata-project/doctrine-orm-admin-bundle": "dev-master#4fcec62fab9487c5479519f5d663c3d1d588a10f",
        "sonata-project/user-bundle": "dev-master#ec09378a100dfe6285562e864c2286ef9104059e",
        "sonata-project/datagrid-bundle": "dev-master#cc42f2801011d7ebc65edda451a2efa4b4d1fbb9",
        "sonata-project/page-bundle": "dev-master#c1ba33adb260e00625ec71542a3e8c5c11ca85ad",
        "sonata-project/block-bundle": "dev-master#9ff5ed97330c59b38d5080e322046f384c36f0e3",
        "sonata-project/seo-bundle": "dev-master#b7b1237228cf90c86ae0a4b4380506facb70982a",
        "sonata-project/formatter-bundle": "dev-master#b854ef71eabc0785fadca2d3dd6b477bf63ca4b8",
        "sonata-project/classification-bundle": "dev-master#06a46292f2651fd5228ec85010944057c6c002d0",
        "sonata-project/media-bundle": "dev-master#d6cb0062cc4f77393959607c2d95137d5f71c57d"

    },
    "require-dev": {
        "sensio/generator-bundle": "~2.3",
        "phpunit/phpunit": "~4.0",
        "fabpot/php-cs-fixer": "~1.0"
    },
    "scripts": {
        "post-root-package-install": [
            "SymfonyStandard\\Composer::hookRootPackageInstall"
        ],
        "post-install-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::removeSymfonyStandardFiles",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget"
        ],
        "post-update-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::removeSymfonyStandardFiles",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget"
        ]
    },
    "config": {
        "bin-dir": "bin"
    },
    "extra": {
        "symfony-app-dir": "app",
        "symfony-web-dir": "web",
        "symfony-assets-install": "relative",
        "incenteev-parameters": {
            "file": "app/config/parameters.yml"
        },
        "branch-alias": {
            "dev-master": "2.7-dev"
        }
    }
}
